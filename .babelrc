{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-3",
    "react"
  ],
  "env": {
    "node": {
      "plugins": [
        [
          "transform-require-ignore",
          {
            "extensions": [
              ".scss"
            ]
          }
        ]
      ]
    }
  },
}
